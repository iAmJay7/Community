<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Creative - Bootstrap 3 Responsive Admin Template">
    <meta name="author" content="GeeksLabs">
    <meta name="keyword" content="Creative, Dashboard, Admin, Template, Theme, Bootstrap, Responsive, Retina, Minimal">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>Creative - Bootstrap Admin Template</title>


    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">

    <link href="~/Content/css/bootstrap-theme.css" rel="stylesheet">

    <link href="~/Content/css/elegant-icons-style.css" rel="stylesheet" />
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />

    <link href="~/Content/css/style.css" rel="stylesheet">
    <link href="~/Content/css/style-responsive.css" rel="stylesheet" />
    <link href="~/Content/css/jquery-ui-1.10.4.min.css" rel="stylesheet">
    @RenderSection("head", required: false)
</head>
<body>
    <section id="container" class="">


        <header class="header dark-bg">
            <div class="toggle-nav">
                <div class="icon-reorder tooltips" data-original-title="Toggle Navigation" data-placement="bottom"><i class="icon_menu"></i></div>
            </div>

            <!--logo start-->
            <a href="index.html" class="logo">Nice <span class="lite">Admin</span></a>
            <!--logo end-->

            <div class="nav search-row" id="top_menu">
                <!--  search form start -->
                <ul class="nav top-menu">
                    <li>
                        <form class="navbar-form">
                            <input class="form-control" placeholder="Search" type="text">
                        </form>
                    </li>
                </ul>
                <!--  search form end -->
            </div>

            <div class="top-nav notification-row">
                <ul class="nav pull-right top-menu">
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <span class="profile-ava">
                                <img alt="" src="img/avatar1_small.jpg">
                            </span>
                            <span class="username">Admin</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <div class="log-arrow-up"></div>

                            <li>
                                <a href="login.html"><i class="icon_key_alt"></i> Log Out</a>
                            </li>

                        </ul>
                    </li>

                </ul>

            </div>
        </header>

        <aside>
            <div id="sidebar" class="nav-collapse ">

                <ul class="sidebar-menu">
                    <li class="active">
                        <a class="" href="index.html">
                            <i class="icon_house_alt"></i>
                            <span>Dashboard</span>
                        </a>
                    </li>
                    <li>
                        <a class="" href="/admin/MassMrg/index">
                            <i class="icon_genius"></i>
                            <span>Mass Marriage</span>
                        </a>
                    </li>
                    <li>
                        <a class="" href="admin/loan/index">
                            <i class="icon_genius"></i>
                            <span>Loan</span>
                        </a>
                    </li>
                </ul>
                <!-- sidebar menu end-->
            </div>
        </aside>
        <section id="main-content">
            <section class="wrapper">

                <div class="row">
                    <a class="col-lg-3 col-md-3 col-sm-12 col-xs-12" href="/Admin/MassMrgReg/Index">
                        <div class="info-box blue-bg">
                            <div class="count">Registraion</div>
                        </div>
                        <!--/.info-box-->
                    </a>
                    <!--/.col-->

                    <a class="col-lg-2 col-md-2 col-sm-12 col-xs-12" href="/Admin/MassMrgGift/Index">
                        <div class="info-box brown-bg">
                            <div class="count">Gifts</div>
                        </div>
                        <!--/.info-box-->
                    </a>
                    <!--/.col-->

                    <a class="col-lg-3 col-md-3 col-sm-12 col-xs-12" href="/Admin/MassMrgCommitties/Index">
                        <div class="info-box dark-bg">
                            <div class="count">Committies</div>
                            <div class="title"></div>
                        </div>
                        <!--/.info-box-->
                    </a>
                    <!--/.col-->

                    <a class="col-lg-2 col-md-2 col-sm-12 col-xs-12" href="/Admin/MassMrgDonarDetail/Index">
                        <div class="info-box green-bg">
                            <div class="count">Donar</div>
                        </div>
                        <!--/.info-box-->
                    </a>
                    <!--/.col-->
                    <a class="col-lg-2 col-md-2 col-sm-12 col-xs-12" href="/Admin/MassMrgExpenditure/Index">
                        <div class="info-box green-bg">
                            <div class="count">Expenditure</div>
                        </div>
                        <!--/.info-box-->
                    </a>
                    
                </div>

            </section>
        </section>
        @RenderBody()

    </section>
    <script src="~/Content/js/jquery.js"></script>
    <script src="~/Content/js/jquery-ui-1.10.4.min.js"></script>
    <script src="~/Content/js/jquery-1.8.3.min.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery-ui-1.9.2.custom.min.js"></script>
    <!-- bootstrap -->
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script>
      //knob
      $(function() {
        $(".knob").knob({
          'draw': function() {
            $(this.i).val(this.cv + '%')
          }
        })
      });

      //carousel
      $(document).ready(function() {
        $("#owl-slider").owlCarousel({
          navigation: true,
          slideSpeed: 300,
          paginationSpeed: 400,
          singleItem: true

        });
      });

      //custom select box

      $(function() {
        $('select.styled').customSelect();
      });

      /* ---------- Map ---------- */
      $(function() {
        $('#map').vectorMap({
          map: 'world_mill_en',
          series: {
            regions: [{
              values: gdpData,
              scale: ['#000', '#000'],
              normalizeFunction: 'polynomial'
            }]
          },
          backgroundColor: '#eef3f7',
          onLabelShow: function(e, el, code) {
            el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
          }
        });
      });
    </script>

</body>
</html>
